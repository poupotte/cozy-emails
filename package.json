{
  "name": "cozy-emails",
  "version": "0.1.0",
  "engines": [
    "node = 0.8.x"
  ],
  "description": "Emails client for Cozycloud",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cozy/cozy-emails.git"
  },
  "main": "server.coffee",
  "dependencies": {
    "americano": "0.3.10",
    "americano-cozy": "0.2.8",
    "printit": "0.1.3",
    "async": "0.9.0",
    "jade": "1.6.0",
    "node-polyglot": "0.4.1",
    "moment": "2.8.2",
    "cozy-fixtures": "1.1.1",
    "bluebird": "2.3.2",
    "imap": "0.8.13",
    "guid": "0.0.12",
    "mailparser": "0.4.6",
    "iconv": "2.1.4",
    "lodash": "2.4.1",
    "request-json": "0.4.12"
  },
  "devDependencies": {
    "should": "4.0.4",
    "mocha": "1.20.1",
    "request-json": "0.4.11",
    "cozy-fixtures": "1.1.1",
    "coffeelint": "^1.6.0",
    "lorem-ipsum": "1.0.1",
    "chai": "1.9.1",
    "coffee-coverage": "0.4.2"
  },
  "scripts": {
    "test": "cake tests",
    "start": "node build/server.js"
  },
  "cozy-permissions": {
    "Account": {
      "description": "Manages your email Accounts"
    },
    "Mailbox": {
      "description": "Manage your Imap Mailboxes"
    },
    "Message": {
      "description": "Manages your emails"
    },
    "CozyInstance": {
      "description": "Localize the application with your language"
    },
    "send mail": {
      "description": "Send emails to your contacts"
    }
  },
  "cozy-displayName": "Emails"
}
